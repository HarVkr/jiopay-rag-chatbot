{
  "version": 2,
  "builds": [
    {
      "src": "rag-chat/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/rag-chat/api/(.*)",
      "dest": "/rag-chat/api/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/rag-chat/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/rag-chat/$1"
    }
  ]
}