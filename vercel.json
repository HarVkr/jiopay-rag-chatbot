{
  "version": 2,
  "builds": [
    {
      "src": "rag_chat/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/rag_chat/$1"
    }
  ],
  "installCommand": "cd rag_chat && npm ci",
  "buildCommand": "cd rag_chat && npm run build",
  "outputDirectory": "rag_chat/.next",
  "framework": "nextjs"
}