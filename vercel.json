{
  "buildCommand": "cd rag-chat && npm run build",
  "outputDirectory": "rag-chat/.next",
  "installCommand": "cd rag-chat && npm ci",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}